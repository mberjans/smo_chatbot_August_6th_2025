[run]
source = lightrag_integration
omit = 
    */tests/*
    */test_*
    */venv/*
    */lightrag_env/*
    */demo_*
    */example_*
    */simple_*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

precision = 2
skip_covered = False
show_missing = True
sort = Cover

[html]
directory = lightrag_integration/coverage_html
title = Clinical Metabolomics Oracle - LightRAG Integration Coverage Report