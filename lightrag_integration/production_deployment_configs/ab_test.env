# A/B Testing Configuration
# Split traffic 50/50 between legacy and production for comparison

PROD_LB_ENABLED=true
PROD_LB_DEPLOYMENT_MODE=a_b_testing
PROD_LB_TRAFFIC_PERCENT=50
PROD_LB_PERF_COMPARISON=true
PROD_LB_AUTO_FAILBACK=true
PROD_LB_ADVANCED_ALGORITHMS=true
PROD_LB_COST_OPTIMIZATION=false
PROD_LB_QUALITY_METRICS=true
PROD_LB_ROLLBACK_ERROR_RATE=5.0
PROD_LB_ROLLBACK_LATENCY_MS=4000.0

# Standard health settings for A/B test
PROD_LB_HEALTH_CHECK_INTERVAL=30
PROD_LB_UNHEALTHY_THRESHOLD=3
PROD_LB_CB_FAILURE_THRESHOLD=5

# Enhanced monitoring for comparison
PROD_LB_ENABLE_MONITORING=true
PROD_LB_MONITORING_INTERVAL=60
PROD_LB_ALERT_ERROR_RATE=2.0
PROD_LB_ALERT_LATENCY_MS=3000

# Enable advanced algorithms for testing
PROD_LB_PRIMARY_ALGORITHM=adaptive
PROD_LB_ENABLE_ADAPTIVE=true
PROD_LB_FALLBACK_ALGORITHMS=least_connections,response_time_based,health_aware,cost_aware