# Full Production Deployment Configuration
# 100% production traffic with all features enabled

PROD_LB_ENABLED=true
PROD_LB_DEPLOYMENT_MODE=production_only
PROD_LB_TRAFFIC_PERCENT=100
PROD_LB_PERF_COMPARISON=false
PROD_LB_AUTO_FAILBACK=true
PROD_LB_ADVANCED_ALGORITHMS=true
PROD_LB_COST_OPTIMIZATION=true
PROD_LB_QUALITY_METRICS=true
PROD_LB_ROLLBACK_ERROR_RATE=5.0
PROD_LB_ROLLBACK_LATENCY_MS=5000.0

# Production-optimized health settings
PROD_LB_HEALTH_CHECK_INTERVAL=30
PROD_LB_UNHEALTHY_THRESHOLD=3
PROD_LB_HEALTHY_THRESHOLD=2
PROD_LB_CB_FAILURE_THRESHOLD=5
PROD_LB_CB_SUCCESS_THRESHOLD=3
PROD_LB_CB_TIMEOUT=300

# Performance optimizations
PROD_LB_MAX_RESPONSE_TIME_MS=3000
PROD_LB_TARGET_RESPONSE_TIME_MS=1500
PROD_LB_MAX_CONCURRENT_REQUESTS=200

# Advanced algorithm configuration
PROD_LB_PRIMARY_ALGORITHM=adaptive
PROD_LB_ENABLE_ADAPTIVE=true
PROD_LB_FALLBACK_ALGORITHMS=least_connections,response_time_based,health_aware,cost_aware,quality_aware

# Production monitoring
PROD_LB_ENABLE_MONITORING=true
PROD_LB_MONITORING_INTERVAL=60
PROD_LB_METRICS_RETENTION_HOURS=720
PROD_LB_ALERT_ERROR_RATE=2.0
PROD_LB_ALERT_LATENCY_MS=2500
PROD_LB_ALERT_QUEUE_SIZE=100

# Cost optimization for production
PROD_LB_COST_TRACKING_ENABLED=true
PROD_LB_DAILY_BUDGET_USD=500.0
PROD_LB_ALERT_THRESHOLD_USD=400.0
PROD_LB_PREFER_LOW_COST_BACKENDS=true
PROD_LB_COST_WEIGHT_FACTOR=0.2

# Production rate limiting
PROD_LB_RATE_LIMIT_RPM=5000
PROD_LB_RATE_LIMIT_BURST=200

# Production logging
PROD_LB_LOG_LEVEL=INFO
PROD_LB_LOG_FORMAT=json
PROD_LB_LOG_FILE=/var/log/production_load_balancer.log
PROD_LB_DEBUG_MODE=false
PROD_LB_TRACE_REQUESTS=false